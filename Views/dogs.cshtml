@{
  Layout="Shared/_Layout.cshtml";
}
<ul id="dropdown1" class="dropdown-content">
    <li><a href="/profile/@Model["customer"].GetId()/newtrip">New Trip</a></li>
    <li class="divider"></li>
    <li><a href="/profile/@Model["customer"].GetId()/trips">Past Trips</a></li>
</ul>
<nav>
    <div class="nav-wrapper">
        <a href="/profile/index/@Model["customer"].GetId()" class="brand-logo">Cabrador</a>
        <ul class="right hide-on-med-and-down">
            <!-- Dropdown Trigger -->
            <li><a class="dropdown-button" href="#!" data-activates="dropdown1">Trips<i class="material-icons right">arrow_drop_down</i></a></li>
            <li><a href="/profile/@Model["customer"].GetId()/dogs">Our Dogs</a></li>
            <li><a href="/profile/@Model["customer"].GetId()">Your Profile</a></li>
            <li><a href="/">Log Out</a></li>
        </ul>
    </div>
</nav>

<div class="puppies">
  <div class="carousel">
    <p class="carousel-item"><img class="carimg" src="/Content/img/Alex.jpg">Alex</p>
    <p class="carousel-item"><img class="carimg" src="/Content/img/Allie.jpg">Allie</p>
    <p class="carousel-item"><img class="carimg" src="/Content/img/Bebe.jpg">Bebe</p>
    <p class="carousel-item"><img class="carimg" src="/Content/img/Bowie.jpg">Bowie</p>
    <p class="carousel-item"><img class="carimg" src="/Content/img/Bella.jpg">Bella</p>
  </div>

</div>

<div class="doggies">
@if (Model["dogs"].Count != 0)
{
  <ul>
    @foreach (var dog in @Model["dogs"])
    {
      <h4 class="dogs"><a href="/profile/@Model["customer"].GetId()/dogs/@dog.GetId()">@dog.GetName()</a></h4>
    }
  </ul>
}
</div>
