@{
  Layout="Shared/_Layout.cshtml";
}


<nav>
    <div class="nav-wrapper">
        <a href="/" class="brand-logo">Cabrador</a>
        <ul class="right hide-on-med-and-down">
            <li><a href="/signup">Sign Up</a></li>
            <li><a href="/login">Login</a></li>
        </ul>
    </div>
</nav>

<div class="row">
    <form action="/welcome/returning" method="post" class="col s12">
        <div class="row">
            <div class="input-field col s12">
                <input id="customer-email" name="customer-email" type="email" class="validate">
                <label for="customer-email">Email</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="customer-password" type="password" name="customer-password" class="validate">
                <label for="customer-password">Password</label>
            </div>
        </div>
        <button type="submit" name="button">Sign Up</button>
    </form>
</div>
