@{
    Layout = "Shared/_Layout.cshtml";
}
<nav>
    <div class="nav-wrapper">
        <a href="/" class="brand-logo">Cabrador</a>
        <ul class="right hide-on-med-and-down">
            <li><a href="/signup">Sign Up</a></li>
            <li><a href="/login">Login</a></li>
        </ul>
    </div>
</nav>

<div class="row dashboard2">
    <img id="dashboard2pic" src="/Content/img/dashboard2.jpeg">

</div>

<div class="row">
  <div class="col s1">

  </div>
    <form action="/welcome/new" method="post" class="col s10 loginform">
      <h3>Sign Up</h3>
        <div class="row">
            <div class="input-field col s6">
                <input id="customer-name" name="customer-name" type="text" class="validate">
                <label for="customer-name">Name</label>
            </div>
            <div class="input-field col s6">
                <input id="customer-photo" type="text" name="customer-photo" class="validate">
                <label for="customer-photo">Photo</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="customer-email" name="customer-email" type="email" class="validate">
                <label for="customer-email">Email</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="customer-password" type="password" name="customer-password" class="validate">
                <label for="customer-password">Password</label>
            </div>
        </div>
        <button type="submit" name="button" class="waves-effect waves-light btn loginbtn">Sign Up</button>
    </form>
</div>
